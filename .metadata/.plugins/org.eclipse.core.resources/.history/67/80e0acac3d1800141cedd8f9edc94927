import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.math.BigInteger;

public class Solution {
	
	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		String input = br.readLine();

		int[] inputNumbers = new int[input.length()];
		
		for(int i=0;i<input.length();i++){
			
			inputNumbers[i] = Integer.parseInt(input.charAt(i)+"");
			
		}
		long time1 = System.currentTimeMillis();
		BigInteger sum = substringNew(inputNumbers);
		long time2 = System.currentTimeMillis();
		System.out.println((sum.mod(new BigInteger(1000000007 + ""))));
		System.out.println((time2-time1)/1000);
	}
	
	public static BigInteger substringNew(int[] inputNumbers){
		
		BigInteger sum = new BigInteger(0 + "");
		int length = inputNumbers.length;
		BigInteger a = new BigInteger(10+"");
		for(int i=0;i<length;i++){
			sum = sum.multiply(a);
			long num = 0 ;
			
			for(int j=0;j<=i;j++){
				
				num += inputNumbers[j] * (j+1);
				
				num = (long)(num % (1000000007));
				
			//	System.out.println(num);
			}
			sum = sum.add(new BigInteger(num + ""));
		}
		return sum;
	}
	
	public static BigInteger subNew(int[] inputNum){
		BigInteger sum = new BigInteger(0 + "");
		int length = inputNum.length;
		//BigInteger a = new BigInteger(10+"");
		int[] array1 = new int[length];
		array1[0] = 1;
		for(int i=1;i<length; i++){
			
			array1[i] = (array1[i-1] * 10 +1) % 1000000007; 
		}
		
		for(int i=0;i<length;i++){
		//	sum = sum.multiply(a);
			long num = 0 ;
			
			
			sum = sum.add(new BigInteger(num + ""));
		}
		return sum;
		
	}
}

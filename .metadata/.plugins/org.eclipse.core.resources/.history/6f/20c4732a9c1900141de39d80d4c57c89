import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.ArrayList;


public class Solution {
	
	public static ArrayList<Long> scores = new ArrayList<>();
	
	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		int t = Integer.parseInt(br.readLine());
		
		for(int i=0; i<t;i++){
			int n = Integer.parseInt(br.readLine());
			
			String line = br.readLine();
			
			String []brickstrings = line.split(" ");
			long[] bricks = new long[brickstrings.length];
			
			for(int j=0;j<brickstrings.length;j++){
				bricks[j] = Long.parseLong(brickstrings[j]);
			}
			playGame(bricks,0,0,true);
			
			System.out.println();
		}
	}
	
	public static void playGame(long[] bricks,long score,int offset,boolean countOrNot){
		
		long score1 = bricks[offset];
		long score2 = bricks[offset] + bricks[offset + 1];
		long score3 = bricks[offset] + bricks[offset + 1] + bricks[offset + 2];
		
		if(offset >= bricks.length){
			scores.add(score);
		}
		
		if(countOrNot){
			score1 += score;
			score2 += score;
			score3 += score;
		}
		
	}
}

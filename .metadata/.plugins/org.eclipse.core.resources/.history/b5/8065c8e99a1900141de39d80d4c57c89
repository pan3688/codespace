import java.io.BufferedReader;
import java.io.InputStreamReader;
import java.util.Arrays;


public class Solution {
	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		int t = Integer.parseInt(br.readLine());
		
		for(int i=0; i<t;i++){
			int n = Integer.parseInt(br.readLine());
			
			String line = br.readLine();
			
			String []brickstrings = line.split(" ");
			long[] bricks = new long[brickstrings.length];
			
			for(int j=0;j<brickstrings.length;j++){
				bricks[j] = Long.parseLong(brickstrings[j]);
			}
			System.out.println(playGame(bricks,0,0,true));
		}
	}
	
	public static long playGame(long[] bricks,long score,int offset,boolean countOrNot){
		
		long score1 = bricks[offset];
		long score2 = bricks[offset] + bricks[offset + 1];
		long score3 = bricks[offset] + bricks[offset + 1] + bricks[offset + 2];
		
		if(offset >= bricks.length)
			return score;
		
		if(countOrNot)
			score = 
		
		return 0L;
	}
}

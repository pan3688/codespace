import java.io.BufferedReader;
import java.io.InputStreamReader;


public class Solution {
	public static void main(String[] args) throws Exception {
		BufferedReader br = new BufferedReader(new InputStreamReader(System.in));
		
		int t = Integer.parseInt(br.readLine());
		
		for(int i=0; i<t;i++){
			int n = Integer.parseInt(br.readLine());
			
			String line = br.readLine();
			
			String []brickstrings = line.split(" ");
			long[] bricks = new long[brickstrings.length];
			
			for(int j=0;j<brickstrings.length;j++){
				bricks[j] = Long.parseLong(brickstrings[j]);
			}
			System.out.println(playGame(bricks, 0));
		}
	}
	
	public static long playGame(long[] bricks,long score){
		
		long score1 = bricks[0];
		long score2 = bricks[0] + bricks[1];
		long score3 = bricks[0] + bricks[1] + bricks[2];
		return 0L;
	}
}
